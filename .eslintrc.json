{
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "no-multiple-empty-lines": 2,
    "no-unused-vars": 1,
    "no-constant-condition": 1,
    "no-debugger": 0,
    "no-control-regex": 0,
    "no-inner-declarations": 1,
    "no-undef": 0,
    "valid-typeof": 1,
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "no-case-declarations": 0
  }
}
